<%- include('partials/header'); -%>



  <!-- Page Header -->
  <header class="masthead" style="background-image: url('images/yag1.png')">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          <div class="site-heading">
            <h1>KuasArt</h1>
            <span class="subheading">Sanat sitemin tasarımı</span>
          </div>
        </div>
      </div>
    </div>

  </header>


         <!--Carousel-->
      <div class="overlay">
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img class="d-block w-100" src="images/moz4.png">
              <div class="carousel-caption d-none d-md-block">
                <h3>Mozaik</h3>
                <p>Belki de en gereksizlerin iktisadını öğrendiğin sanattır mozaik</p>
              </div>
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="images/yag2.png">
              <div class="carousel-caption d-none d-md-block">
                <h3>Yağlı Boya</h3>
                <p>Hata yapsan da elbet geri dönüşü vardır, bir yağlı boya dokunuşu kadar kolay olmasada</p>
              </div>
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="images/sulu1.png">
              <div class="carousel-caption d-none d-md-block">
                <h3>Sulu Boya</h>
                <p>Sudur can veren, sanat da olsa</p>
              </div>
            </div>
          </div>
          <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>

  <%- include('partials/footer'); -%>
